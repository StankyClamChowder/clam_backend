{
  "env": {
    "es6": true,
    "node": true,
    "mocha": true,
  },
  "plugins": [
    "react"
  ]
  "extends": "fullstack",
  "rules": {
    "indent": [
      "error",
      2
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ]
  }
}
